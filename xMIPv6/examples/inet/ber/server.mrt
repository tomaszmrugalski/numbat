# filename: server.mrt
# author: Thomas Reschka
# routing table for server of tcp example "ber"

ifconfig:

# interface 0 to server
name: ppp0
	inet_addr: 172.0.0.2
	Mask: 255.255.255.0
	MTU: 1500
	POINTTOPOINT MULTICAST

ifconfigend.


route:

#Destination	Gateway         Genmask         Flags 	Metric	Iface
172.0.0.1   	172.0.0.2   	255.255.255.0	H   	0   	ppp0

routeend.
